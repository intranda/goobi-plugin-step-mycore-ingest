<config_plugin>
    <!--
        order of configuration is:
          1.) project name and step name matches
          2.) step name matches and project is *
          3.) project name matches and step name is *
          4.) project name and step name are *
	-->
    
    <config>
    
        <!-- which projects to use for (can be more then one, otherwise use *) -->
        <project>*</project>
        <step>*</step>
        
        <!-- URL to use for the Transformation of METS into the MyCoRe object xml -->
        <!-- 
        <xslt-url>https://zs-test.thulb.uni-jena.de/xsl/goobimets.xsl</xslt-url>
        -->
        <xslt-url>http://localhost/goobimets.xsl</xslt-url>
        
        
        <!-- Parameters for the MyCoRe REST API -->
        <mycore-api>https://mycore.de/api/v2/</mycore-api>
        <mycore-login>login</mycore-login>
        <mycore-password>password</mycore-password>
        
    </config>

</config_plugin>
